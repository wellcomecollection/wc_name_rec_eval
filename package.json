{
  "name": "aws-amplify-gen2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:sandbox": "npx ampx sandbox --profile platform-admin-lon",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:name-rec": "tsx scripts/uploadNameRec.ts",
    "seed:name-rec:sandbox": "tsx scripts/uploadNameRec.ts --directory data/name_rec --clearData",
    "seed:name-rec:deployment": "tsx scripts/uploadNameRec.ts --outputs amplify_outputs.deployment.main.json --directory data/name_rec --clearData",
    "list:appsync": "aws appsync list-graphql-apis --region $AWS_REGION --profile $AWS_PROFILE --query 'graphqlApis[].{name:name,id:apiId,url:uris.GRAPHQL}'",
    "tail:amplify": "tsx scripts/tailAmplifyLogs.ts --step Build",
    "tail:amplify:list": "tsx scripts/tailAmplifyLogs.ts --list",
    "tail:amplify:choose": "tsx scripts/tailAmplifyLogs.ts --interactive"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.5.5",
    "@aws-sdk/client-appsync": "^3.620.0",
    "@aws-sdk/credential-providers": "^3.620.0",
    "aws-amplify": "^6.6.6",
    "next": "14.2.10",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.1",
    "@aws-amplify/backend-cli": "^1.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "add": "^2.0.6",
    "aws-cdk": "^2",
    "aws-cdk-lib": "^2",
    "constructs": "^10.3.0",
    "dotenv": "^17.2.1",
    "esbuild": "^0.23.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
